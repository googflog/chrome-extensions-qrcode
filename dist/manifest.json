{
  "manifest_version": 2,
  "name": "Quick Get QR & Short URL",
  "version": "1.3",
  "browser_action": {
    "default_icon": {
        "19": "images/icon19.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "clipboardWrite",
    "storage"
  ],
  "content_security_policy":"script-src 'self' https://api-ssl.bitly.com; object-src 'self'"
}
